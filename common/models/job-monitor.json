{
  "name": "jobMonitor",
  "base": "PersistedModel",
  "idInjection": true,
  "indexes": {
    "idx_uniq": {
      "keys": {
        "start_time": 1,
        "end_time": 1,
        "featurizer": 1,
        "lang": 1
      },
      "options": {
        "unique": true
      }
    }
  },
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "Timestamp": {},
    "DestroyAll": {}
  },
  "properties": {
    "run_set_id":{
      "type":"string",
      "description": "Used to match up all job monitors run for a specific time window"
    },
    "start_time": {
      "type": "number",
      "required": true,
      "description": "query start time (NOT job start time)"
    },
    "end_time": {
      "type": "number",
      "required": true,
      "description": "query end time (NOT job end time)"
    },
    "featurizer": {
      "type": "string",
      "required": true
    },
    "priority":{
      "type":"string",
      "default":"normal"
    },
    "lang": {
      "type": "string"
    },
    "state": {
      "type": "string",
      "required": true,
      "default": "new"
    },
    "done_at": {
      "type": "date"
    },
    "run_attempt_count": {
      "type": "number",
      "default":0
    },
    "error_msg": {
      "type": "string"
    },
    "start": {
      "type": "boolean",
      "required": true,
      "description": "trigger the monitoring process",
      "default": false
    },
    "service_args": {
      "type": "object",
      "description": "service-specific arguments, i.e. similarity_threshold",
      "default": {}
    }
  },
  "validations": [],
  "relations": {
    "postsClusters": {
      "type": "hasMany",
      "model": "postsCluster",
      "foreignKey": "job_monitor_id"
    }
  },
  "acls": [],
  "methods": {}
}
